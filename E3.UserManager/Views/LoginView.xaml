                <!--#region Password Error box-->
                <Grid Grid.Row="6" Background="#89373c">
                    <Grid.Style>
                        <Style TargetType="{x:Type Grid}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding PasswordError}" Value="True">
                                    <Setter Property="Visibility" Value="Visible"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding PasswordError}" Value="False">
                                    <Setter Property="Visibility" Value="Hidden"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=PasswordTextBox,Path=IsKeyboardFocused}" Value="True">
                                    <Setter Property="Visibility" Value="Hidden"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=UsernameTextBox,Path=IsKeyboardFocused}" Value="True">
                                    <Setter Property="Visibility" Value="Hidden"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Grid.Style>
                    <Viewbox>
                        <Label Content="{Binding PasswordErrorMessage}" Style="{StaticResource ErrorLabelStyle}"/>
                    </Viewbox>
                </Grid>
                <!--#endregion-->

                <!--#region Login Button--> 
                <Grid Grid.Row="8">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="1" Content="Login" Command="{Binding LoginCommand}" CommandParameter="{Binding ElementName=PasswordTextBox}" Style="{StaticResource LoginButtonStyle}"/>
                </Grid>
                <!--#endregion-->
            </Grid>
            <Grid Grid.Row="1" Background="#1d2532" Visibility="{Binding AuthenticationInProgress, Converter={StaticResource BoolToVis}}">
                <userControls:LoadingPanel Background="Transparent" VerticalAlignment="Center" ShowCloseButton="False" IsLoading="True" Message="Authentication is in Progress. Please Wait......."/>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
